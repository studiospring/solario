<%%= form_for @<%= singular_table_name %>, :validate => true do |f| %>
  <%%= render 'shared/error_messages', object: f.object %>

  <% attributes.each do |attribute| %>
    <%%= f.label :<%= attribute.name %> %><br />
    <%%= f.<%= attribute.field_type %> :<%= attribute.name %> %>
  <% end %>
  <%%= f.submit %>
<%% end %>
