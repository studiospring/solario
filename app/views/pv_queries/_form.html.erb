<%= form_for @pv_query, :validate => true do |f| %>
  <%= render 'shared/error_messages', object: f.object %>
  <%= f.label :postcode_id, 'Postcode', min: '221', max: '9999' %>
  <%= f.number_field :postcode_id %>
  <%= f.fields_for :panels do |builder| %>
    <%= render 'panel_fields', f: builder %>
  <% end %>
  <%= link_to 'add panel', '', class: 'btn add_fields', remote: true %>
  <%= f.submit 'view output', class: "btn btn-primary" %>
<% end %>
